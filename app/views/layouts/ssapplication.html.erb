<script type="text/javascript">
  $(function() {
    function setSpeaker(speaker, tr) {
      if (speaker) {
        tr.find('.speaker_id').val(speaker.id);
      }
      else {
        tr.find('.speaker_id').val('');
      }
    }

    function autocompleteSpeaker(tr) {
      $(tr).find('.speaker_name').autocomplete({
                        source: "/events/1/speakers.json",
                        minLength: 2,
                        select: function(event, ui) {
                          setSpeaker(ui.item, tr);
                        }
      });
     }

    function addSpeaker(tr) {
      var newSpeaker;
      if (tr.is('.speaker')) {
        tr.after(constructRow());
        newSpeaker = tr.next('tr');
      }
      else {
        tr.before(constructRow());
        newSpeaker = tr.prev('tr');
      }
      blankSlate();
      autocompleteSpeaker(newSpeaker);
      newSpeaker.find('.speaker_name:first').focus();
    }

    function removeSpeaker(elm) {
      var item_id = $(elm).siblings('input.id').val();
      var tr = $(elm).parents('tr:first');
      if (item_id && item_id != '') {
        tr.find('input.mark_delete').val('true');
        tr.hide();
      }
      else {
        tr.remove();
      }
      blankSlate();
    }

    function blankSlate() {
      if ($('#speakers > tr.speaker:visible').length == 0) {
        $('thead').hide();
        $('#no_items').show();
      }
      else {
        $('thead').show();
        $('#no_items').hide();
      }
    }

    function constructRow() {
      return '<tr class="speaker">' +
        '<td>' +
          '<input class="speaker_name" name="agenda_speaker[][speaker_name]" type="text" value="" />' +
          '<input class="speaker_id" name="agenda_speaker[][event_speaker_id]" type="hidden" value="" />' +
        '</td>' +

        '<td>' +
          '<input class="speaker_remarks" name="agenda_speaker[][remarks]" type="text" value="" />' +
        '</td>' +

        '<td>' +
          '<img title="Remove" class="delete" src="/images/trash-s.png" />' +
          '<input name="agenda_speaker[][id]" type="hidden" class="id" />' +
          '<input name="agenda_speaker[][mark_delete]" type="hidden" class="mark_delete" />' +
        '</td>' +
      '</tr>';
    }

    blankSlate();
    $(".speaker").each(function() {
      autocompleteSpeaker($(this));
    });

    $('#speakers > tr.speaker').find('img.delete').live('click', function() {
      removeSpeaker(this);
    });

    $('.add_speaker').click(function(event) {
      event.preventDefault();
      addSpeaker($(this).parents('tr:first'));
    });

  });
</script>

